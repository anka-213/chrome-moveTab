{
  "name": "Keyboard Tab Mover",
  "version": "0.1",
  "description": "Creates keyboard shortcuts (Ctrl + Left/Right/Up/Down) to move currently selected tab to adjacent windows",
  "background": {
    "persistent": false,
    "scripts": ["background.js", "createNewWindow.js"]
  },
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "browser_action": {
    "default_icon": {
             "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png"
    }
  },
  "commands": {
    "move-left": {
      "suggested_key": { "default": "Ctrl+Left" },
      "description": "Move tab one window left"
    },
    "move-right": {
      "suggested_key": { "default": "Ctrl+Right" },
      "description": "Move tab one window right"
    },
    "move-up": {
      "suggested_key": { "default": "Ctrl+Up" },
      "description": "Move tab one window up"
    },
    "move-down": {
      "suggested_key": { "default": "Ctrl+Down" },
      "description": "Move tab one window down"
    }
  },
  "permissions": [
      "storage"
  ],
  "manifest_version": 2
}
